{
  "outConfig": {
    "plugin": "wiewsOutputCSV"
  },
  "flow": [
    {
      "name": "wiews_area_filter",
      "sid": [ { "uid": "wiews_region_mapping" },{ "uid": "wiews_region_countries" } ],
      "rid" : { "uid" : "area_selection" },
      "result" : false,
      "parameters": {
        "total" : false,
        "filter" : {
          "m49": {
            "codes": [
              {
                "uid": "wiews_m49_regions",
                "codes": [ "WITC","1" ]
              }
            ]
          }
        }
      }
    },

    {
      "name": "filter",
      "sid": [ { "uid": "raw_indicator10" }, { "uid": "area_selection" } ],
      "parameters": {
        "rows": {
          "iteration": {
            "codes": [
              {
                "uid": "wiews_iteration",
                "codes": [ "1" ]
              }
            ]
          },
          "country_iso3" : {
            "variable" : "required_countries"
          }
        }
      }
    },
    {
      "name": "order",
      "parameters": {
        "country" : "ASC",
        "organization_name" : "ASC"
      }
    },
    {
      "name": "columns",
      "parameters" : {
        "columns" : [
          "iteration",
          "country",
          "organization_name",
          "sites_total",
          "sites_with_management"
        ]
      }

    }
  ]
}
