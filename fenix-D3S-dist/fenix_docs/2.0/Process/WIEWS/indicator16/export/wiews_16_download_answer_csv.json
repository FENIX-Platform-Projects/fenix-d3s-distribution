{
  "outConfig": {
    "plugin": "wiewsOutputCSV"
  },
  "flow": [
    {
      "name": "wiews_area_filter",
      "sid": [ { "uid": "wiews_region_mapping" },{ "uid": "wiews_region_countries" } ],
      "rid" : { "uid" : "area_selection" },
      "result" : false,
      "parameters": {
        "total" : false,
        "filter" : {
          "m49": {
            "codes": [
              {
                "uid": "wiews_m49_region_only",
                "codes": [ "1" ]
              }
            ]
          }
        }
      }
    },

    {
      "name": "filter",
      "sid": [ { "uid": "raw_indicator16" }, { "uid": "area_selection" } ],
      "parameters": {
        "rows": {
          "iteration": {
            "codes": [
              {
                "uid": "wiews_iteration",
                "codes": [ "1" ]
              }
            ]
          },
          "country_iso3" : {
            "variable" : "required_countries"
          }
        }
      }
    },
    {
      "name": "order",
      "parameters": {
        "iteration" : "ASC",
        "country" : "ASC",
        "org_name" : "ASC",
        "crop_name" : "ASC"
      }
    },
    {
      "name": "columns",
      "parameters" : {
        "columns" : [
          "iteration",
          "country",
          "org_name",
          "mission_id",
          "mission_name",
          "mission_name_local",
          "start_date",
          "end_date",
          "crop_id",
          "crop_name",
          "crop_name_local",
          "taxon_id",
          "taxon_name_local",
          "accessions_number",
          "secured_accessions_number",
          "area_id",
          "area_name",
          "area_name_local"
        ]
      }
    }
  ]
}
