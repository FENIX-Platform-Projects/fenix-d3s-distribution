{
  "outConfig": {
    "plugin": "wiewsOutputCSV"
  },
  "flow": [
    {
      "name": "wiews_area_filter",
      "sid": [ { "uid": "wiews_region_mapping" },{ "uid": "wiews_region_countries" } ],
      "rid" : { "uid" : "area_selection" },
      "result" : false,
      "parameters": {
        "filter" : {
          "iso3": {
            "codes": [
              {
                "uid": "ISO3",
                "codes": [ "DEU", "ALB" ]
              }
            ]
          }
        }
      }
    },

    {
      "name": "filter",
      "sid": [ { "uid": "raw_indicator20" }, { "uid": "area_selection" } ],
      "parameters": {
        "rows": {
          "iteration": {
            "codes": [
              {
                "uid": "wiews_iteration",
                "codes": [ "1" ]
              }
            ]
          },
          "country_iso3" : {
            "variable" : "required_countries"
          }
        }
      }
    },
    {
      "name":"order",
      "parameters":{
        "iteration":"ASC",
        "country":"ASC",
        "stakeholder":"ASC",
        "taxon_freetext":"ASC",
        "crop_freetext":"ASC"
      }
    },
    {
      "name": "columns",
      "parameters" : {
        "columns" : [
          "iteration",
          "country",
          "stakeholder",
          "holdinginstitutecode",
          "accessionno",
          "taxonid",
          "taxon_freetext",
          "cropid",
          "crop_freetext",
          "acquisitiondate",
          "origincountry",
          "biologicalaccession",
          "genebankid",
          "genebank_freetext",
          "latitude",
          "longitude",
          "germplasmastore",
          "multilateralsystemstatus"
        ]
      }
    }

  ]
}
