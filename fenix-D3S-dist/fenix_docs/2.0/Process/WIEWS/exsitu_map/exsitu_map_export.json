{
  "outConfig": {
    "plugin": "wiewsOutputCSV"
  },
  "flow": [
    {
      "sid": [ { "uid": "exsitu_institutes_count" } ],
      "name": "filter",
      "parameters": {
        "rows": {
          "year" : {
            "time" : [{"from" : 2016, "to" : 2016}]
          }
        }
      }
    },
    {
      "name":"order",
      "parameters":{
        "year":"ASC",
        "country_iso3":"ASC",
        "instcode":"ASC"
      }
    },
    {
      "name": "columns",
      "parameters": {
        "columns": [
          "year",
          "country_iso3",
          "instcode",
          "latitude",
          "longitude",
          "accessions_count",
          "genus_count",
          "genus_species_count",
          "um"
        ]
      }
    }
  ]
}
