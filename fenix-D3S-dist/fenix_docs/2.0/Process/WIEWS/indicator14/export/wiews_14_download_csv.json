{
  "outConfig" : {
    "plugin" : "outputCSV"
  },
  "flow" : [
    {
      "name": "wiews_area_filter",
      "sid": [ { "uid": "wiews_region_mapping" },{ "uid": "wiews_region_countries" } ],
      "rid" : { "uid" : "area_selection" },
      "result" : false,
      "parameters": {
        "filter" : {
          "iso3": {
            "codes": [
              {
                "uid": "wiews_m49_region_only",
                "codes": [ "ALB", "BGD" ]
              }
            ]
          }
        }
      }
    },

    {
      "sid": [ { "uid": "indicator14" }, { "uid": "area_selection" } ],
      "name": "filter",
      "parameters": {
        "columns": [
          "domain",
          "wiews_region",
          "rank",
          "indicator",
          "element",
          "stakeholder",
          "iteration",
          "value",
          "um"
        ],
        "rows": {

          "iteration": {
            "codes": [
              {
                "uid": "wiews_iteration",
                "codes": [ "1" ]
              }
            ]
          },
          "wiews_region" : {
            "variable" : "required_countries"
          },
          "element": {
            "codes": [
              {
                "uid": "wiews_elements",
                "codes": [ "ind", "nfp", "nfpa", "stk" ]
              }
            ]
          }
        }
      }
    },

    {
      "name":"addcolumn",
      "parameters":{
        "column":{
          "dataType":"text",
          "id":"indicator_label",
          "title":{
            "EN":"Indicator"
          }
        },
        "value": {
          "keys": [
            "element = 'stk'", "element <> 'stk'"
          ],
          "values": [
            "@@direct 'Indicator (' || stakeholder || ' / ' || stakeholder_en || ')'","element_en"
          ]
        }
      }
    },
    {
      "name": "order",
      "parameters": {
        "rank" : "ASC",
        "wiews_region" : "ASC"
      }
    }

  ]
}
