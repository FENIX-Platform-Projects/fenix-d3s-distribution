[
  {
    "name": "filter",
    "parameters": {
      "rows": {
        "recipientcode": {
          "codes": [
            {
              "uid": "crs_recipients",
              "version": "2016",
              "codes": [
                "625"
              ]
            }
          ]
        },
        "donorcode": {
          "codes": [
            {
              "uid": "crs_donors",
              "version": "2016",
              "codes": [
                "1"
              ]
            }
          ]
        },
        "oda": {
          "enumeration": [
            "usd_commitment"
          ]
        },
        "value": {
          "number": [
            {
              "from": 0.00001
            }
          ]
        },
        "year": {
          "time": [
            {
              "from": "2000",
              "to": "2015"
            }
          ]
        }
      },
      "columns": [
        "year",
        "value",
        "unitcode",
        "donorcode",
        "recipientcode"
      ]
    }
  },
  {
    "name": "group",
    "parameters": {
      "by": [
        "year",
        "donorcode",
        "recipientcode"
      ],
      "aggregations": [
        {
          "columns": [
            "value"
          ],
          "rule": "SUM"
        },
        {
          "columns": [
            "unitcode"
          ],
          "rule": "MAX"
        }
      ]
    }
  }
]