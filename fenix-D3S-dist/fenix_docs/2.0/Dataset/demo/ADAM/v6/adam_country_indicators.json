{
  "uid": "adam_country_indicators",
  "title": {
    "EN": "ADAM Recipient/Donor Country Indicators data"
  },
  "meContent": {
    "description": {
      "EN": "Global recipient and donor country indicators from various sources. Includes last available year and calculated averages."
    },
    "resourceRepresentationType": "dataset"
  },
  "dsd": {
    "contextSystem": "adam",
    "datasources": ["adam"],
    "cache":{
      "storage":"postgres"
    },
    "columns": [

      {
        "id": "countryname",
        "title": {
          "EN": "Country name"
        },
        "dataType": "text"
      },
      {
        "id": "countrycode",
        "subject": "geo",
        "title": {
          "EN": "Country code"
        },
        "dataType": "code",
        "domain": {
          "codes": [{
            "idCodeList": "crs_recipientdonors",
            "version": "2016",
            "level": 1
          }]
        },
        "key": true
      },
      {
        "id": "indicatorname",
        "title": {
          "EN": "Indicator name"
        },
        "dataType": "text"
      },
      {
        "id": "indicatorcode",
        "title": {
          "EN": "Indicator code"
        },
        "dataType": "code",
        "domain": {
          "codes": [{
            "idCodeList": "adam_development_indicators",
            "version": "2016",
            "level": 1
          }]
        },
        "key": true
      },
      {
        "id": "period",
        "title": {
          "EN": "Period"
        },
        "dataType": "text",
        "key": true
      },
      {
        "id": "value",
        "subject": "value",
        "title": {
          "EN": "Value"
        },
        "dataType": "number"
      },
      {
        "id": "unitcode",
        "subject": "um",
        "title": {
          "EN": "Measurement unit"
        },
        "domain": {
          "codes": [{
            "idCodeList": "crs_units",
            "version": "2016",
            "level": 1
          }]
        },
        "dataType": "code"
      },
      {
        "id": "unitname",
        "title": {
          "EN": "Measurement unit name"
        },
        "dataType": "text"
      },
      {
        "id": "source",
        "title": {
          "EN": "Source"
        },
        "dataType": "text"
      },
      {
        "id": "note",
        "title": {
          "EN": "Note"
        },
        "dataType": "text"
      },
      {
        "id": "internalnote",
        "title": {
          "EN": "Internal note"
        },
        "dataType": "text"
      }

    ]
  }
}