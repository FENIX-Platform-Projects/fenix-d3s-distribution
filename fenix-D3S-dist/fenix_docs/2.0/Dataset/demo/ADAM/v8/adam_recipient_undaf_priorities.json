        "title" : { "EN" : "From Year" },
        "dataType" : "text",
	"key": true
      }, 
      {
        "id" : "to_year",
        "title" : { "EN" : "To Year" },
        "dataType" : "text",
	"key": true
      },
      {
        "id" : "stated_priority",
        "title" : { "EN" : "UNDAF Stated Priority" },
        "dataType" : "text",
        "key": true
      },
      {
        "id" : "purposecode",
        "title" : { "EN" : "OCED DAC" },
        "dataType" : "code",
        "domain" : { "codes" : [ { "idCodeList" : "crs_purposes", "version" : "2016", "level" : 1 } ] } ,
        "key": true
